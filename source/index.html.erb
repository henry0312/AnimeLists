---
title: アニメリスト
---

<ol class="breadcrumb">
  <li class="active">HOME</li>
</ol>

<div class="navigation">
<% get_years.each do |year| %>
  <h2><%= "#{year}年アニメ" %></h2>
  <ul class="nav nav-pills">
    <% get_quarters(year).each do |quarter| %>
      <li class="dropdown nav-item">
        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
          <%= "#{seasons[quarter.to_i]}アニメ" %> <span class="caret"></span>
        </a>
        <ul class="dropdown-menu" role="menu">
          <li><%= link_to "タイトル一覧", "/#{year}/Q#{quarter}/titles.html" %></li>
          <li><%= link_to "音楽", "/#{year}/Q#{quarter}/music.html" %></li>
          <li class="divider"></li>
          <% locations.each do |key, value| %>
            <li><%= link_to value, "/#{year}/Q#{quarter}/#{key}.html" %></li>
          <% end %>
        </ul>
      </li>
    <% end %>
  </ul>
<% end %>
</div>

<div class="links">
  <h2>リンク</h2>
  <ul>
    <li><a href="https://github.com/henry0312/AnimeLists" target="_blank">henry0312/AnimeLists</a></li>
    <li><a href="http://cal.syoboi.jp/" target="_blank">しょぼいカレンダー</a></li>
    <li><a href="http://uzurainfo.han-be.com/" target="_blank">うずらインフォ</a></li>
  </ul>
</div>
